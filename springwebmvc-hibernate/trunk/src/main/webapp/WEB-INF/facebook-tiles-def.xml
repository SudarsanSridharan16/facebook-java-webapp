<!DOCTYPE tiles-definitions PUBLIC
"-//Apache Software Foundation//DTD Tiles Configuration 1.1//EN"
"http://jakarta.apache.org/struts/dtds/tiles-config_1_1.dtd">

<tiles-definitions>
    <definition name="header" page="/WEB-INF/content/tiles/main-header.jsp" />
    <definition name="default" page="/WEB-INF/content/tiles/default-layout.jsp">
        <put name="header" value="header" />
        <put name="body" value="/WEB-INF/content/500.htm" />
    </definition>
    <definition name="welcome" extends="default">
        <put name="body" value="/WEB-INF/content/welcome.jsp" />
    </definition>
    <definition name="invite" extends="default">
        <put name="body" value="/WEB-INF/content/invite.jsp" />
    </definition>
    <definition name="fbmlTags" extends="default">
        <put name="body" value="/WEB-INF/content/fbmlTags.jsp" />
    </definition>
    <definition name="redirect" extends="default">
        <put name="body" value="/WEB-INF/content/redirect.jsp" />
    </definition>
    <definition name="blank" page="/WEB-INF/content/tiles/blank.jsp" />
</tiles-definitions>
