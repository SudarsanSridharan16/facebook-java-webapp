<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"
        "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
<urlrewrite>
	<rule>
		<from>^/$</from>
		<to type="forward">/welcome.fbml</to>
	</rule>
	<rule>
		<from>^([^?]*)/([^?/\.]+)(\?.*)?$</from>
		<to last="true">$1/$2.fbml$3</to>
	</rule>
	<outbound-rule>
		<from>^(.*)\.fbml(\?.*)?$</from>
		<to last="false">$1$2</to>
	</outbound-rule>
</urlrewrite>